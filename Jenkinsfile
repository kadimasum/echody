pipeline{
    
    agent any
    
    stages{
        
        stage("clone"){
            steps{
                echo "Cloning repository"
            }
        }
        
        stage("Test"){
            steps{
                echo "Testing code"
            }
        }
        
        stage("Deploy"){
            steps{
                echo "Deploying code"
            }
        }
        
    }
}
